<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cpfei.project">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ImageListActivity" />
        <activity
            android:name=".activity.material.ToolBarActivity"
            android:label="ToolBar"
            android:theme="@style/AppThemeBase" />
        <activity
            android:name=".activity.MaterialDesignActivity"
            android:label="MaterialDesign" />
        <activity
            android:name=".activity.material.DrawerLayoutActivity"
            android:theme="@style/AppThemeNoTitleBar" />
        <activity
            android:name=".activity.material.CoordinatorLayoutActivity"
            android:theme="@style/AppThemeNoTitleBar" />
    </application>

</manifest>